# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
# 서버 전용 키 (절대 클라이언트에 노출 금지) - AI 비서 API 등 서버사이드에서 사용
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Gemini (AI 비서 전체)
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.5-flash

# 네이버 웍스 API
NAVER_WORKS_CLIENT_ID=
NAVER_WORKS_CLIENT_SECRET=
NAVER_WORKS_REDIRECT_URI=http://localhost:3001/api/naver-works/oauth/callback
# 메일 발송자 ID: 네이버 웍스 사용자 ID 또는 'me' (OAuth 인증된 사용자 본인)
NAVER_WORKS_MAIL_SENDER=me
NAVER_WORKS_SCOPE=mail

# (Legacy: Service Account JWT, Mail API에는 권장하지 않음)
NAVER_WORKS_SERVICE_ACCOUNT=
NAVER_WORKS_PRIVATE_KEY=
NAVER_WORKS_PRIVATE_KEY_PATH=
